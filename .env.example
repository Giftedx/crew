# Copy to .env and fill with your secrets
OPENAI_API_KEY=your-openai-key
# Alternative provider via OpenRouter
OPENROUTER_API_KEY=your-openrouter-key
DISCORD_WEBHOOK=https://discord.com/api/webhooks/your-webhook
DISCORD_PRIVATE_WEBHOOK=https://discord.com/api/webhooks/your-private-webhook
GOOGLE_CREDENTIALS=./google-credentials.json
# Base directory for downloads, logs and temp files (defaults to ~/crew_data)
CREWAI_BASE_DIR=./crew_data
# Optional Qdrant connection for vector memory
QDRANT_URL=http://localhost:6333
